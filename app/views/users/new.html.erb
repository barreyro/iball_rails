<div class="small-8 small-offset-2 medium-6 medium-offset-3 large-offset-3 large-6 columns" id="newuserform">
Create an account
  <%= form_for User.new do |f| %>
    <%= f.text_field :email, placeholder: "Email" %>
    <%= f.text_field :username, placeholder: "Username" %>
    <%= f.password_field :password, placeholder: "Password" %>
    <%= f.text_field :city, placeholder: "City" %>
    <%= f.text_field :state, placeholder: "State" %>
    <%= f.text_field :age, placeholder: "Age" %>
    <%= f.text_field :bio, placeholder: "Team" %>
    <%= submit_tag 'signup', class: "button tiny" %>
  <% end %>
</div>
