<div class="row">
  <nav class="small-4 medium-4 large-4 columns" id="navlog">
     <% if session[:user_id] %>
      <a href="/logout">logout </a>
      <% else %>
      Created by Steph
     <% end %>
  </nav>

  <nav class="small-4 medium-4 large-4 columns" id="navlogo"><center>
    <%= link_to image_tag("redball.png", :border=>0, :class => 'navlogo'),
:action => 'index', :controller => 'welcome'%></center>
  </nav>

  <nav class="small-4 medium-4 large-4 columns" id="navlink">
   <% if !session[:user_id] %>
      <a href="/signup">Signup</a>
   <% else %>
     <%= link_to current_user.username, current_user %>
   <% end %>
  </nav>
</div>